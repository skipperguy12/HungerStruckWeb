<h1>Registrations#createuser</h1>
<p>Find me in app/views/registrations/createuser.html.erb</p>
<% if not @status == nil and @status = 0 %>
<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <%= hidden_field_tag 'uuid', @user.uuid %>

  <div><%= f.label :password %>
  <%= f.password_field :password, :autofocus => true %></div>

  <div><%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %></div>

  <div><%= f.submit "Sign up", :class => 'btn btn-primary' %></div>
<% end %>
<% else %>
	<%= "Error, no valid token and/or username provided!" %>
<% end %>
